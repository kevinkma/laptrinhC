#include <stdio.h>
#include <conio.h>
void main(void)
{
  float diem;
  printf("nhap diem: ");
  scanf("%f",&diem);
  if (diem >=0 && diem <=10)
    if (diem >=9)
     printf("xep loai xuat sac");
    else if (diem >=8)
     printf("xep loai gioi");
    else if (diem >=7)
     printf("xep loai kha");
    else if (diem >=6)
     printf("xep loai trung binh kha");
    else if (diem >=5) 
     printf("xep loai trung binh");
    else 
     printf("xep loai yeu");
  else
   printf("ban nhap diem ko dung!");
  getch();
}
